<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="faq">

	<!-- 신규알림수조회 -->
	<select id="selectFaqList" parameterType="map" resultType="FaqListDataVo">
		<![CDATA[ /* SQL_ID : faq.selectFaqList */ ]]>
		select 
			faq_no,
			faq_question,
			faq_answer
		from 
			w_faq
		order by faq_no asc
	</select>

</mapper>


